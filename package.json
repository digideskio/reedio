{
  "name": "reedio",
  "version": "0.0.1",
  "description": "music discovery platform powered by youtube and echonest",
  "license": "MIT",
  "author": "Kurt Weiberth",
  "repository": {
    "type": "git",
    "url": "git@github.com:kweiberth/reedio.git"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "webpack && node .",
    "start:dev": "npm run css:dev & webpack --watch & nodemon . --watch",
    "up": "docker-compose -f docker-compose-dev.yml up --build",
    "up:d": "docker-compose -f docker-compose-dev.yml up -d --build",
    "up:prod": "docker-compose up --build",
    "up:prod:d": "docker-compose up -d --build",
    "css:dev": "npm rebuild node-sass && npm run font-awesome && node-sass public/sass/main.scss dist/css/main.css --watch public/sass",
    "font-awesome": "cp node_modules/font-awesome/css/font-awesome.min.css dist/css/font-awesome.min.css && mkdir -p dist/fonts && cp -R node_modules/font-awesome/fonts/* dist/fonts"
  },
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.1",
    "classnames": "^2.1.3",
    "es6-promisify": "^4.1.0",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "font-awesome": "^4.4.0",
    "jquery": "^3.1.0",
    "keymirror": "^0.1.1",
    "node-sass": "^3.8.0",
    "random-global": "^1.0.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-slider": "^0.7.0",
    "react-youtube": "^7.0.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "spotify-web-api-node": "^2.3.5",
    "underscore": "^1.8.3",
    "webpack": "^1.13.1",
    "youtube-node": "^1.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.3.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.1.0",
    "nodemon": "^1.10.0"
  }
}
